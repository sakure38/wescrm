/**app.wxss**/
.container {
  min-height: 100vh;
  background: #fff;
  box-sizing: border-box;
}
.ux-text{
  margin-top: 6rpx;
}
.ux-price{
  float: right;
  color:#bf0008;
  margin-right: 4rpx;
}
.full-image{
  width: 100%;
}
page::before{
  content: '';
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 2rpx;
  background-color: #F6F8FC;
  z-index: 9999;
}

